{
  "name": "Move Mail Shortcuts",
  "description": "Allows to assign keyboard shortcuts for moving mails to folders.",
  "version": "4",
  "applications": {
    "gecko": {
      "id": "mailMoveShortcuts@extension",
      "strict_min_version": "78.0"
    }
  },
  "homepage_url": "https://github.com/robert7k/moveMailShortcuts",
  "icons": {
	"48": "img/48.png"
  },
  "permissions": [
    "messagesMove",
    "messagesRead",
    "accountsRead",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "move_mail_0": {
      "suggested_key": {
        "default": "Alt+1"
      },
      "description": "Folder 1"
    },
    "move_mail_1": {
      "suggested_key": {
        "default": "Alt+2"
      },
      "description": "Folder 2"
    },
    "move_mail_2": {
      "suggested_key": {
        "default": "Alt+3"
      },
      "description": "Folder 3"
    },
    "move_mail_3": {
      "suggested_key": {
        "default": "Alt+4"
      },
      "description": "Folder 4"
    },
    "move_mail_4": {
      "suggested_key": {
        "default": "Alt+5"
      },
      "description": "Folder 5"
    },
    "move_mail_5": {
      "suggested_key": {
        "default": "Alt+6"
      },
      "description": "Folder 6"
    },
    "move_mail_6": {
      "suggested_key": {
        "default": "Alt+7"
      },
      "description": "Folder 7"
    },
    "move_mail_7": {
      "suggested_key": {
        "default": "Alt+8"
      },
      "description": "Folder 8"
    },
    "move_mail_8": {
      "suggested_key": {
        "default": "Alt+9"
      },
      "description": "Folder 9"
    },
    "move_mail_9": {
      "suggested_key": {
        "default": "Alt+0"
      },
      "description": "Folder 10"
	}
  },
  "options_ui": {
    "page": "options.html"
  },
  "manifest_version": 2
}
