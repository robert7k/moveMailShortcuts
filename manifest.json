{
  "name": "Move Mail Shortcuts",
  "description": "Allows to assign keyboard shortcuts for moving mails to folders.",
  "version": "5",
  "applications": {
    "gecko": {
      "id": "mailMoveShortcuts@extension",
      "strict_min_version": "78.0"
    }
  },
  "homepage_url": "https://github.com/robert7k/moveMailShortcuts",
  "icons": {
	"48": "img/48.png"
  },
  "permissions": [
    "messagesMove",
    "messagesRead",
    "accountsRead",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "commands": {
    "move_mail_0": {
      "suggested_key": {
        "default": "Alt+Shift+1"
      },
      "description": "Move to folder 1"
    },
    "move_mail_1": {
      "suggested_key": {
        "default": "Alt+Shift+2"
      },
      "description": "Move to folder 2"
    },
    "move_mail_2": {
      "suggested_key": {
        "default": "Alt+Shift+3"
      },
      "description": "Move to folder 3"
    },
    "move_mail_3": {
      "suggested_key": {
        "default": "Alt+Shift+4"
      },
      "description": "Move to folder 4"
    },
    "move_mail_4": {
      "suggested_key": {
        "default": "Alt+Shift+5"
      },
      "description": "Move to folder 5"
    },
    "move_mail_5": {
      "suggested_key": {
        "default": "Alt+Shift+6"
      },
      "description": "Move to folder 6"
    },
    "move_mail_6": {
      "suggested_key": {
        "default": "Alt+Shift+7"
      },
      "description": "Move to folder 7"
    },
    "move_mail_7": {
      "suggested_key": {
        "default": "Alt+Shift+8"
      },
      "description": "Move to folder 8"
    },
    "move_mail_8": {
      "suggested_key": {
        "default": "Alt+Shift+9"
      },
      "description": "Move to folder 9"
    },
    "move_mail_9": {
      "suggested_key": {
        "default": "Alt+Shift+0"
      },
      "description": "Move to folder 10"
	},
    "goto_0": {
      "suggested_key": {
        "default": "Ctrl+Alt+1"
      },
      "description": "Go to folder 1"
    },
    "goto_1": {
      "suggested_key": {
        "default": "Ctrl+Alt+2"
      },
      "description": "Go to folder 2"
    },
    "goto_2": {
      "suggested_key": {
        "default": "Ctrl+Alt+3"
      },
      "description": "Go to folder 3"
    },
    "goto_3": {
      "suggested_key": {
        "default": "Ctrl+Alt+4"
      },
      "description": "Go to folder 4"
    },
    "goto_4": {
      "suggested_key": {
        "default": "Ctrl+Alt+5"
      },
      "description": "Go to folder 5"
    },
    "goto_5": {
      "suggested_key": {
        "default": "Ctrl+Alt+6"
      },
      "description": "Go to folder 6"
    },
    "goto_6": {
      "suggested_key": {
        "default": "Ctrl+Alt+7"
      },
      "description": "Go to folder 7"
    },
    "goto_7": {
      "suggested_key": {
        "default": "Ctrl+Alt+8"
      },
      "description": "Go to folder 8"
    },
    "goto_8": {
      "suggested_key": {
        "default": "Ctrl+Alt+9"
      },
      "description": "Go to folder 9"
    },
    "goto_9": {
      "suggested_key": {
        "default": "Ctrl+Alt+0"
      },
      "description": "Go to folder 10"
	}
  },
  "options_ui": {
    "page": "options.html"
  },
  "manifest_version": 2
}
